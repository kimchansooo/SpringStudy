	public ModelAndView notice() {
		return null;
	}
	
	@RequestMapping("/customer/noticeDetail.do")
	public ModelAndView noticeDetail(@RequestParam(value="seq",defaultValue="") String seq) {
		
		return null;
	}
	
}
